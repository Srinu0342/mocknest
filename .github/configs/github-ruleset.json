{
    "name": "Default Safety Rules",
    "target": "branch",
    "enforcement": "active",
    "conditions": {
      "ref_name": {
        "include": ["refs/heads/main"],
        "exclude": []
      }
    },
    "rules": [
      { "type": "deletion" },
      { "type": "non_fast_forward" },
      { "type": "pull_request", 
        "parameters": {
          "required_approving_review_count": 1,
          "require_code_owner_review": true
        }
      }
    ]
  }